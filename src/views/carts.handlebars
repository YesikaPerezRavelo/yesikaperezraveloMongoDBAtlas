<div class="container">
  <h2>All Carts</h2>
  <ul>
    {{#each carts}}
      <li>
        <strong>Cart ID:</strong>
        {{this._id}}<br />
        <strong>Products:</strong>
        <ul>
          {{#each this.products}}
            <li>{{this.product.name}} - Quantity: {{this.quantity}}</li>
          {{/each}}
        </ul>
      </li>
    {{/each}}
  </ul>
</div>

<script src="/socket.io/socket.io.js"></script>
{{! <script src="/js/index.js"></script> }}